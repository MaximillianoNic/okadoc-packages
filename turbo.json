{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib/**"]
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": []
    },
    "tsc": {
      "dependsOn": ["^tsc"],
      "outputs": [
        "lib/**"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}